<div class="row">
    <div class="col-md-12">
        <div class="card">
          <div class="card-header no-border">
            <div class="d-flex justify-content-between">
              <a href="/blocklist/create" class="btn btn-primary">New blockList</a>
              <a href="javascript:void(0);">View Report</a>
            </div>
          </div>
          <div class="card-body">
            <table class="table">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Identifier</th>
                        <th>Period</th>
                        <th>BanUsers</th>
                        <th>Action</th>

                    </tr>
                </thead>
                <tbody>
                    {{#each list}}
                    <tr>
                        <td>{{ this._id }}</td>
                        <td>{{ this.identifier}}</td>
                        <td>{{ this.period}}</td>
                        <td>{{ this.banUsers}}</td>
                        <td>
                            <a href="/blocklist/edit/{{ this._id }}" class="btn btn-default">Edit</a>
                            <a href="/blocklist/delete/{{ this._id}}" class="btn btn-default">Delete</a>
                        </td>
                    </tr>     
                    {{/each}}
                </tbody>
            </table>
          </div>
        </div>
     </div>
</div>